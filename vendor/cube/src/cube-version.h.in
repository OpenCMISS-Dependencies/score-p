#ifndef CUBE_VERSION_H
#define CUBE_VERSION_H


/*-------------------------------------------------------------------------*/
/**
 *  @file  cube-version.h
 *  @brief Provides the Cube package version information.
 **/
/*-------------------------------------------------------------------------*/


/// @name Cube version information
/// @{

/// Cube revision number
#define CUBE_REVISION "@CUBE_BUILD_REVISION@"

/// Cube revision number
#define CUBE_REVISION_NUMBER @CUBE_BUILD_REVISION@

/// Cube revision number
#define CUBE_REVISION_STRING CUBE_REVISION




/// Cube major version number
#define CUBE_MAJOR   "@PACKAGE_MAJOR@"

/// Cube minor version number
#define CUBE_MINOR   "@PACKAGE_MINOR@"

/// Cube bugfix version number
#define CUBE_BUGFIX   "@PACKAGE_BUGFIX@"

/// Cube suffix
#define CUBE_SUFFIX   "@PACKAGE_SUFFIX@"

/// Cube suffix_internally
#define CUBE_SUFFIX_INTERNAL   @PACKAGE_SUFFIX@

/// Cube version number (<i>major</i>.<i>minor</i>)
#define CUBE_SHORT_VERSION   CUBE_MAJOR "." CUBE_MINOR

/// Cube full version number (<i>major</i>.<i>minor</i>.<i>bugfix</i>)
#define CUBE_FULL_VERSION   CUBE_SHORT_VERSION "." CUBE_BUGFIX

/// Cube version number
#define CUBE_VERSION "@CUBE_BUILD_VERSION@"

/// Cube package name "cube"
#define CUBE_SHORT_NAME  "@CUBE_BUILD_SHORT_NAME@"

/// Cube package name "cube-4.x.1-XX"
#define CUBE_FULL_NAME   CUBE_SHORT_NAME "-" CUBE_FULL_VERSION CUBE_SUFFIX 


/// Cube library interface information
#define LIBRARY_CURRENT "@LIBRARY_CURRENT@"
#define LIBRARY_REVISION "@LIBRARY_REVISION@"
#define LIBRARY_AGE "@LIBRARY_AGE@"

/// @}


#endif   // !CUBE_VERSION_H
